{
  "name": "gabriela",
  "version": "1.0.4",
  "description": "Plugin based framework",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "engines": {
    "node": ">=8.*"
  },
  "scripts": {
    "test": "mocha --bail -R mocha-pretty-spec-reporter 'tests/**/*.js'",
    "coverage": "nyc --print=both -x '**/tests/*' -x '**/src/gabriela/testing/*' --reporter=lcov npm run test",
    "console-coverage": "nyc --print=both -x '**/tests/*' -x '**/src/gabriela/testing/*' --reporter=text npm run test",
    "lint": "eslint src/gabriela/**"
  },
  "author": "Mario Skrlec",
  "license": "MIT",
  "dependencies": {
    "deasync": "^0.1.19",
    "deepcopy": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "strict-mode": "^1.1.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "express-react-views": "^0.11.0",
    "faker": "^4.1.0",
    "mocha": "^6.2.3",
    "mocha-pretty-spec-reporter": "0.1.0-beta.2",
    "nyc": "^14.1.1",
    "pem": "^1.14.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabriela-framework/gabriela.git"
  },
  "keywords": [
    "framework",
    "nodejs",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/gabriela-framework/gabriela/issues"
  },
  "homepage": "https://gabriela-framework.github.io/gabriela/#/"
}
