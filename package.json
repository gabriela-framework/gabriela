{
  "name": "gabriela",
  "version": "0.4.0-alpha.0",
  "description": "Plugin based framework",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha -R mocha-pretty-spec-reporter 'tests/**/*.js'",
    "coverage": "nyc -x=test --print=both --exclude=**/tests/* --reporter=lcov npm run test",
    "console-coverage": "nyc -x=test --print=both --exclude=**/tests/* --reporter=text npm run test",
    "lint": "eslint src/gabriela/**"
  },
  "author": "Mario Skrlec",
  "license": "MIT",
  "dependencies": {
    "deasync": "^0.1.15",
    "deepcopy": "^2.0.0",
    "restify": "^8.4.0",
    "strict-mode": "^1.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "faker": "^4.1.0",
    "mocha": "^6.2.0",
    "mocha-pretty-spec-reporter": "0.1.0-beta.2",
    "nyc": "^14.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
