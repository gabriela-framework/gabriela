{
  "name": "gabriela",
  "version": "0.6.1",
  "description": "Plugin based framework",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha --bail -R mocha-pretty-spec-reporter 'tests/**/*.js'",
    "coverage": "nyc --print=both -x '**/tests/*' -x '**/src/gabriela/testing/*' --reporter=lcov npm run test",
    "console-coverage": "nyc --print=both -x '**/tests/*' -x '**/src/gabriela/testing/*' --reporter=text npm run test",
    "lint": "eslint src/gabriela/**"
  },
  "author": "Mario Skrlec",
  "license": "MIT",
  "dependencies": {
    "deasync": "^0.1.15",
    "deepcopy": "^2.0.0",
    "dotenv": "^8.0.0",
    "restify": "^8.4.0",
    "strict-mode": "^1.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "faker": "^4.1.0",
    "mocha": "^6.2.0",
    "mocha-pretty-spec-reporter": "0.1.0-beta.2",
    "node-mocks-http": "^1.8.0",
    "nyc": "^14.1.1",
    "pem": "^1.14.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabriela-framework/gabriela.git"
  },
  "keywords": [
    "framework",
    "nodejs",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/gabriela-framework/gabriela/issues"
  },
  "homepage": "https://github.com/gabriela-framework/gabriela#readme"
